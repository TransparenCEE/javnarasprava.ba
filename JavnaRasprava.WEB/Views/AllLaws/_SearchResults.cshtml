@model PagedList.IPagedList<LawSummaryModel>


<div class="clearfix">
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong><i class="fa fa-book"></i> @GlobalLocalization.Global_Laws</strong>
        </div>
        <br />
        <div class="grid clearfix" data-masonry='{ "itemSelector": ".law-box" }'>

        @if ( Model.Count == 0 )
        {
            <h5>@GlobalLocalization.Global_SearchResults_NoResultsMessage</h5>
        }

        @foreach ( var item in Model )
        {
            @Html.Partial( "_lawBox", item );
        }

        </div>
    </div>
</div>


