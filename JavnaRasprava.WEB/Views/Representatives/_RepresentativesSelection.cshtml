@model RepresentativeListModel

<div class="col-lg-12">
    @foreach ( var parliamentHouse in Model.ParliamentHouses )
    {
        <h1>@parliamentHouse.Name</h1>
        <div class="col-lg-12">
            @foreach ( var representative in parliamentHouse.Representatives )
            {
                <div class="col-lg-3">
                    @Html.Partial( "_RepresentativeQuickSelectionView", representative )
                </div>
            }
        </div>
    }
</div>
