@model List<JavnaRasprava.WEB.Models.Law.RepresentativeQuestionEditModel>

<h3>@GlobalLocalization.Admin_UnverifiedDirectQuestions_Heading</h3>

@if ( Model.Count() > 0 )
{
    <div>
        @foreach ( var question in Model )
        {
            <div>@Html.ActionLink( question.Text, "EditRepresentativeQuestion", new { questionId = question.QuestionID } )</div>
                <br />
        }

    </div>
}
else
{
    <p>@GlobalLocalization.Admin_Unverified_NothingHere</p>
}
