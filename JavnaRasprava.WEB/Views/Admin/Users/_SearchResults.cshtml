@model PagedList.IPagedList<JavnaRasprava.WEB.Models.User.UserModel>
@using PagedList.Mvc;

<table class="table table-striped">
    <tr>
        <th> @GlobalLocalization.User_UserName </th>
        <th> @GlobalLocalization.User_EmailConfirmed </th>
        <th> @GlobalLocalization.User_IsDisabled </th>
        <th> @GlobalLocalization.User_IsAdmin </th>
        <th> @GlobalLocalization.User_AccessFailedCount </th>
        <th> @GlobalLocalization.Account_DemographyAge </th>
        <th> @GlobalLocalization.Account_DemographyEducation </th>
        <th> @GlobalLocalization.Account_DemographyLocation </th>
        <th> @GlobalLocalization.Account_DemographyParty </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td> @Html.DisplayFor(modelItem => item.UserName) </td>
            <td> @Html.DisplayFor(modelItem => item.EmailConfirmed) </td>
            <td> @Html.DisplayFor(modelItem => item.IsDisabled) </td>
            <td> @Html.DisplayFor(modelItem => item.IsAdmin) </td>
            <td> @Html.DisplayFor(modelItem => item.AccessFailedCount) </td>
            <td> @Html.DisplayFor(modelItem => item.AgeValue) </td>
            <td> @Html.DisplayFor(modelItem => item.EducationValue) </td>
            <td> @Html.DisplayFor(modelItem => item.Location) </td>
            <td> @Html.DisplayFor(modelItem => item.Party) </td>
            <td> @Html.ActionLink(GlobalLocalization.Admin_Global_Details, "UserDetails", new { id = item.Id }) </td>
        </tr>
    }
</table>
