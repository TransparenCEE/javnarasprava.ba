@model PagedList.IPagedList<LawSectionSummaryModel>


<div class="clearfix">
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong><i class="fa fa-lightbulb-o"></i> @GlobalLocalization.Global_LawSections</strong>
        </div>
        <br />

        @if ( Model.Count == 0 )
        {
            <h5>@GlobalLocalization.Global_SearchResults_NoResultsMessage</h5>
        }

        @foreach ( var item in Model )
        {
            @Html.Partial( "_lawSectionBox", item );
        }

        <div class="clearfix"></div>
        <br />
    </div>
</div>


